{
  "paper_id": "P03",
  "repo_name": "google-research/google-research",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n gr-env python=3.8 -y",
      "micromamba activate gr-env",
      "micromamba install -c conda-forge -c pytorch -c nvidia -c defaults -y pytorch torchvision torchaudio pytorch-cuda=11.7 cudatoolkit=11.3 pip"
    ]
  },
  "steps": [
    {
      "id": "install_dependencies",
      "description": "Install dependencies",
      "working_dir": ".",
      "commands": [
        "pip install -r speculative_kd/requirements.txt",
        "pip install -r action_gap_rl/requirements.txt",
        "pip install -r multi_annotator/requirements.txt",
        "pip install -r tip/requirements.txt",
        "pip install -r camp_zipnerf/requirements.txt"
      ],
      "expected_artifacts": [
        "speculative_kd",
        "action_gap_rl",
        "multi_annotator",
        "tip",
        "camp_zipnerf"
      ]
    },
    {
      "id": "download_data",
      "description": "Download data",
      "working_dir": ".",
      "commands": [
        "wget https://github.com/google-research/google-research/archive/refs/heads/main.zip",
        "unzip main.zip"
      ],
      "expected_artifacts": [
        "google-research-main.zip",
        "google-research-main"
      ]
    },
    {
      "id": "run_main_experiment",
      "description": "Run main experiment",
      "working_dir": "google-research-main/speculative_kd",
      "commands": [
        "bash run.sh"
      ],
      "expected_artifacts": [
        "main experiment output"
      ]
    }
  ]
}