{
  "paper_id": "P03",
  "repo_name": "google-research/google-research",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n gr_env -y",
      "micromamba activate gr_env",
      "micromamba install -c conda-forge python=3.8 -y",
      "micromamba install -c conda-forge pytorch torchvision torchaudio -c pytorch -y",
      "micromamba install -c conda-forge cudatoolkit=11.3 -y",
      "pip install git+https://github.com/NVIDIA/apex.git"
    ]
  },
  "steps": [
    {
      "id": "install_dependencies",
      "description": "Install dependencies",
      "working_dir": ".",
      "commands": [
        "micromamba run -n gr_env pip install -r speculative_kd/requirements.txt",
        "micromamba run -n gr_env pip install -r action_gap_rl/requirements.txt",
        "micromamba run -n gr_env pip install -r multi_annotator/requirements.txt",
        "micromamba run -n gr_env pip install -r camp_zipnerf/requirements.txt"
      ],
      "expected_artifacts": [
        "gr_env"
      ]
    },
    {
      "id": "download_data",
      "description": "Download data",
      "working_dir": ".",
      "commands": [
        "git clone https://github.com/google-research-datasets/Google-Lens-Dataset.git",
        "git clone https://github.com/google-research-datasets/Action-Gap-Dataset.git",
        "git clone https://github.com/google-research-datasets/Multi-Annotator-Dataset.git",
        "git clone https://github.com/google-research-datasets/Camp-ZipNerf-Dataset.git"
      ],
      "expected_artifacts": [
        "Google-Lens-Dataset",
        "Action-Gap-Dataset",
        "Multi-Annotator-Dataset",
        "Camp-ZipNerf-Dataset"
      ]
    },
    {
      "id": "run_experiment",
      "description": "Run the main experiment",
      "working_dir": "speculative_kd",
      "commands": [
        "bash run.sh"
      ],
      "expected_artifacts": [
        "experiment_results"
      ]
    }
  ]
}