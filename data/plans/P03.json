{
  "paper_id": "P03",
  "repo_name": "google-research/google-research",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n experiments python=3.9 -y",
      "micromamba install -n experiments -c conda-forge -c pytorch torch==1.10.0 torchvision==0.11.2 -y",
      "micromamba install -n experiments -c conda-forge numpy scipy scikit-learn pandas matplotlib -y",
      "micromamba install -n experiments -c pytorch torchaudio -y",
      "micromamba install -n experiments -c conda-forge torchtext -y",
      "micromamba install -n experiments -c pip setuptools<60.0.0 wheel<0.37.0"
    ]
  },
  "steps": [
    {
      "id": "Data Preparation",
      "description": "Download and preprocess data",
      "working_dir": "./fm4tlp",
      "commands": [
        "git clone https://github.com/google-research/google-research.git --depth=1",
        "cd fm4tlp",
        "python utils/pre_process.py"
      ],
      "expected_artifacts": [
        "processed_data.zip"
      ]
    },
    {
      "id": "Model Training",
      "description": "Train the JODIE model",
      "working_dir": "./fm4tlp",
      "commands": [
        "python train.py --config config/jodie.pbtxt"
      ],
      "expected_artifacts": [
        "model_trained.pth"
      ]
    }
  ]
}