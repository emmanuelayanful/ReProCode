{
  "paper_id": "P03",
  "repo_name": "google-research/google-research",
  "env": {
    "type": "conda",
    "setup_commands": [
      "conda create -n reproducibility-environment python=3.9",
      "conda activate reproducibility-environment",
      "conda install -c conda-forge micromamba",
      "micromamba install -c pytorch torchvision"
    ]
  },
  "steps": [
    {
      "id": "Install Dependencies",
      "description": "Install required packages using Conda",
      "working_dir": ".",
      "commands": [
        "conda env update -n reproducibility-environment",
        "pip install -r speculative_kd/requirements.txt",
        "pip install -r action_gap_rl/requirements.txt"
      ],
      "expected_artifacts": []
    },
    {
      "id": "Data Preparation",
      "description": "Assuming data is downloaded, prepare the necessary directories",
      "working_dir": "speculative_kd",
      "commands": [
        "mkdir -p data"
      ],
      "expected_artifacts": []
    },
    {
      "id": "Train SpeculativeKD",
      "description": "Run the SFT training script",
      "working_dir": "speculative_kd/train",
      "commands": [
        "python run_kd_train.py --config config/sft/sft_config_gsm.yaml"
      ],
      "expected_artifacts": [
        "modelCheckpoint"
      ]
    },
    {
      "id": "Action Gap RL Demo",
      "description": "Run the demo for Action Gap RL",
      "working_dir": "action_gap_rl/demo.py",
      "commands": [
        "python demo.py"
      ],
      "expected_artifacts": [
        "results.csv"
      ]
    }
  ]
}