{
  "paper_id": "P01",
  "repo_name": "siegelz/core-bench",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n core-bench python=3.9 -y",
      "micromamba install -n core-bench -c conda-forge numpy pandas torch torchvision transformers scikit-learn requests",
      "pip install -r requirements.txt"
    ]
  },
  "steps": [
    {
      "id": "Install Dependencies",
      "description": "Set up the environment with required packages",
      "working_dir": ".",
      "commands": [
        "source activate core-bench",
        "pip install -r requirements.txt"
      ],
      "expected_artifacts": []
    },
    {
      "id": "Download Data",
      "description": "If data is provided, download the CORE-Bench dataset",
      "working_dir": "benchmark",
      "commands": [
        "wget https://corebench.cs.princeton.edu/capsules/capsule-XXXXXXX.tar.gz -O core_train.tar.gz",
        "tar -xzf core_train.tar.gz"
      ],
      "expected_artifacts": [
        "core_train.tar.gz"
      ]
    },
    {
      "id": "Run Experiment",
      "description": "Run the main experiment using the harness",
      "working_dir": "core-bench",
      "commands": [
        "python3 main.py --experiment_name <experiment_name> --agent_dir agents/AutoGPT-CORE --dataset_file benchmark/dataset/core_test.json --no_gpu --task_limit <task_limit> --benchmark_level <benchmark_level> --agent_script coreagent_hard_gpt4o.sh --verbose"
      ],
      "expected_artifacts": [
        "report.json"
      ]
    }
  ]
}