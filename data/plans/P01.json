{
  "paper_id": "P01",
  "repo_name": "siegelz/core-bench",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n core-bench python=3.9",
      "micromamba activate core-bench",
      "micromamba install -c conda-forge -y --file requirements.txt"
    ]
  },
  "steps": [
    {
      "id": "install_dependencies",
      "description": "Install dependencies",
      "working_dir": ".",
      "commands": [
        "micromamba install -c conda-forge -y --file requirements.txt"
      ],
      "expected_artifacts": [
        "All dependencies installed"
      ]
    },
    {
      "id": "decrypt_dataset",
      "description": "Decrypt dataset",
      "working_dir": "benchmark/dataset",
      "commands": [
        "gpg --output core_test.json --decrypt core_test.json.gpg"
      ],
      "expected_artifacts": [
        "core_test.json"
      ]
    },
    {
      "id": "run_benchmark",
      "description": "Run benchmark",
      "working_dir": ".",
      "commands": [
        "python3 main.py --experiment_name test_coreagent_gpt4o_c-4 --agent_dir agents/AutoGPT-CORE --dataset_file benchmark/dataset/core_test.json --no_gpu --task_limit 1 --benchmark_level codeocean_hard --agent_script coreagent_hard_gpt4o.sh --verbose"
      ],
      "expected_artifacts": [
        "report.json",
        "agent_trace.log"
      ]
    }
  ]
}