{
  "paper_id": "P01",
  "repo_name": "core-bench",
  "env": {
    "type": "conda",
    "setup_commands": [
      "conda create --name core-bench python=3.9",
      "conda activate core-bench",
      "pip3 install -r requirements.txt"
    ]
  },
  "steps": [
    {
      "id": "Install Dependencies",
      "description": "Install required packages for CORE-Bench",
      "working_dir": ".",
      "commands": [
        "conda env update",
        "pip install -r requirements.txt"
      ],
      "expected_artifacts": []
    },
    {
      "id": "Download Data",
      "description": "Download CORE-Bench dataset (if not already present)",
      "working_dir": "benchmark",
      "commands": [
        "gpg --output benchmark/dataset/core_test.json --decrypt benchmark/dataset/core_test.json.gpg"
      ],
      "expected_artifacts": [
        "core_test.json"
      ]
    },
    {
      "id": "Run Experiment",
      "description": "Run the CORE-Bench agents on a task (example with AutoGPT-CORE)",
      "working_dir": "core-bench",
      "commands": [
        "python3 main.py",
        "--experiment_name test_coreagent_gpt4o_c-4",
        "--agent_dir agents/AutoGPT-CORE",
        "--dataset_file benchmark/dataset/core_test.json",
        "--no_gpu",
        "--task_limit 1",
        "--benchmark_level codeocean_hard",
        "--agent_script coreagent_hard_gpt4o.sh",
        "--verbose"
      ],
      "expected_artifacts": [
        "report.json"
      ]
    }
  ]
}