{
  "paper_id": "P06",
  "repo_name": "salesforce/CodeT5",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n codet5_env python=3.9 -y",
      "micromamba install -n codet5_env pip>=21.3.1",
      "micromamba install -n codet5_env torch torchvision transformers scikit-learn numpy pandas",
      "micromamba install -n codet5_env git",
      "pip install --upgrade setuptools wheel"
    ]
  },
  "steps": [
    {
      "id": "Install Dependencies",
      "description": "Install required packages using Conda",
      "working_dir": "/path/to/repo",
      "commands": [
        "source activate codet5_env",
        "pip install -r requirements.txt"
      ],
      "expected_artifacts": [
        "dependency_package_list.txt"
      ]
    },
    {
      "id": "Prepare Data (if applicable)",
      "description": "Download or preprocess data (if not provided)",
      "working_dir": "/path/to/repo",
      "commands": [
        "python CodeT5/run_download_data.py"
      ],
      "expected_artifacts": [
        "data_downloaded.zip"
      ]
    },
    {
      "id": "Run Main Experiment",
      "description": "Execute the main experiment using run_multi_gen.py",
      "working_dir": "/path/to/repo",
      "commands": [
        "source activate codet5_env",
        "python CodeT5/run_multi_gen.py --config path/to/config/file"
      ],
      "expected_artifacts": [
        "output_files.tar.gz"
      ]
    }
  ]
}