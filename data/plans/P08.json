{
  "paper_id": "P08",
  "repo_name": "mem0ai/mem0",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n mem0 python=3.9 -y",
      "micromamba install -n mem0 pip -y",
      "micromamba run -n mem0 pip install -U pip setuptools wheel -y",
      "micromamba run -n mem0 pip install -r requirements.txt"
    ]
  },
  "steps": [
    {
      "id": "install_dependencies",
      "description": "Install the project dependencies",
      "working_dir": ".",
      "commands": [
        "micromamba run -n mem0 pip install -r requirements.txt"
      ],
      "expected_artifacts": [
        "requirements.txt"
      ]
    },
    {
      "id": "run_main_experiment",
      "description": "Run the main experiment",
      "working_dir": ".",
      "commands": [
        "micromamba run -n mem0 python main.py"
      ],
      "expected_artifacts": [
        "main.py"
      ]
    }
  ]
}