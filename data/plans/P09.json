{
  "paper_id": "P09",
  "repo_name": "Kinto/kinto",
  "env": {
    "type": "conda",
    "setup_commands": [
      "micromamba create -n kinto python=3.10 -y",
      "micromamba install -n kinto pip -y",
      "micromamba run -n kinto pip install -r requirements.txt"
    ]
  },
  "steps": [
    {
      "id": "install_dependencies",
      "description": "Install the project dependencies",
      "working_dir": ".",
      "commands": [
        "micromamba run -n kinto pip install -r requirements.txt"
      ],
      "expected_artifacts": [
        "kinto/bin/kinto"
      ]
    },
    {
      "id": "run_main_experiment",
      "description": "Run the main experiment",
      "working_dir": ".",
      "commands": [
        "micromamba run -n kinto python app.wsgi"
      ],
      "expected_artifacts": [
        "Server running on http://localhost:8888"
      ]
    }
  ]
}